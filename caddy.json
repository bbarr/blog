{
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "listen": [ ":443" ],
          "routes": [
            {
              "handle": [
                { 
                  "handler": "reverse_proxy", 
                  "upstreams": [
                    { "dial": "localhost:3000" }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}
